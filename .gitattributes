# Stuff I didn't write
share/dict/en_*   linguist-vendored

# Machine-generated files
bookmarklets.html linguist-generated

# Documentation
share/man/**      linguist-documentation

# Unrecognised filetypes
*.manpath         linguist-language=INI
*.colorscheme     linguist-language=INI
Shell.profile     linguist-language=INI
sessionui.rc      linguist-language=XML
konsoleui.rc      linguist-language=XML
pro.terminal      linguist-language=XML
terminalrc        linguist-language=INI
etc/shells        linguist-language=INI
etc/*.conf        linguist-language=INI
**/ublock/white*  linguist-language=Hosts
calendar*         linguist-language=C linguist-detectable=false

# Adobe Illustrator settings
[attr]aiprefs linguist-language=PostScript linguist-vendored
Adobe*Prefs       aiprefs
Last*Settings     aiprefs
Vector*Presets    aiprefs
**/Workspaces/*   aiprefs
*.aia             aiprefs
*.kys             aiprefs
*.kys_mac         aiprefs

# Mozilla LZ4-compressed files
[attr]mozlz4 linguist-generated diff=mozlz4
*.baklz4          mozlz4
*.jsonlz4         mozlz4
*.json.lz4        mozlz4
*.mozlz4          mozlz4
upgrade.jsonlz4-* mozlz4

# Mozilla preferences file
**/firefox/*.js   linguist-detectable=false
