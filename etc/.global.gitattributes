# Image diffs
*.ai      diff=exif
*.gif     diff=exif
*.jpg     diff=exif
*.jpeg    diff=exif
*.png     diff=exif
*.psd     diff=exif

# Document diffs
*.eps     diff=ps
*.doc     diff=doc
*.docx    diff=docx
*.odt     diff=odt
*.pdf     diff=pdf

# Symbol/library diffs
*.[ao]    diff=sym
*.so      diff=sym
*.dylib   diff=dyld
