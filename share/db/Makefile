# Create offline copies of important repositories and databases
all: IANA IMDb ISBN MDN MIME-Types Public-Suffix-List RFCs Schemas SCOWL

IANA:; @$(inc); cmd rsync -avz --delete rsync://rsync.iana.org/assignments IANA/
RFCs:; @$(inc); cmd rsync -avz --delete rsync.rfc-editor.org::rfcs-text-only RFCs/
Schemas:; @$(mirrorGit) 'git@github.com:schemaorg/schemaorg.git'
MIME-Types:; @$(mirrorGit) 'git@github.com:mime-types/mime-types-data.git'
Public-Suffix-List:; @$(mirrorGit) 'git@github.com:publicsuffix/list.git'

# SpellCheck-Oriented WordLists
SCOWL: SCOWL/Dictionaries SCOWL/wordlist
SCOWL/Dictionaries:; @$(mirrorGit) 'https://anongit.freedesktop.org/git/libreoffice/dictionaries.git'
SCOWL/wordlist:;     @$(mirrorGit) 'git@github.com:en-wl/wordlist.git'

# Mozilla's web-technology docs
MDN: MDN/browser-compat-data MDN/content MDN/data MDN/shared-assets
MDN/browser-compat-data:; @$(mirrorGit) 'git@github.com:mdn/browser-compat-data.git'
MDN/content:;             @$(mirrorGit) 'git@github.com:mdn/content.git'
MDN/data:;                @$(mirrorGit) 'git@github.com:mdn/data.git'
MDN/shared-assets:;       @$(mirrorGit) 'git@github.com:mdn/shared-assets.git'

# IMDb non-commercial datasets
IMDb:
	@$(inc); test -d $@ || cmd mkdir $@; \
	cmd wget -P $@ -nd -crA '*.tsv,*.gz' 'https://datasets.imdbws.com/'

# ISBN publisher's registry
ISBN: ISBN-Ranges.xml
ISBN-Ranges.xml:
	@$(inc); \
	cmd wget -O $@ 'https://www.isbn-international.org/export_rangemessage.xml'

# RDF prefixes list
XML-Namespaces.tsv:
	@$(inc); for cmd in \
		"printf 'Prefix\tURL\n' > $@" \
		"curl -k 'https://prefix.cc/popular/all.file.txt' | sort >> $@" \
	; do cmdfmt "$$cmd" && eval "$$cmd"; done

# GitHub's Atom
Atom: Atom/atom
Atom/Pulsar: Atom/atom
	@$(mirrorGit) 'git@github.com:pulsar-edit/pulsar.git'; \
	git remote add atom "`realpath $^`"
