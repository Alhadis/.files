[user]
	name = Alhadis
	email = gardnerjohng@gmail.com
	signingkey = C57044A83594344A
[core]
	editor = /usr/local/bin/emacs
	excludesfile = ~/.files/etc/.gitignore-global
	attributesfile = ~/.files/etc/.gitattributes-global
	whitespace = -blank-at-eol
	ignorecase = false
	untrackedCache = true
[alias]
	co = checkout
	r = remote --verbose
	s = status
[credential]
	helper = osxkeychain
[diff]
	renames = copies
[diff "bin"]
	textconv = hexdump -v -C
	cachetextconv = true
[diff "exif"]
	textconv = exiftool -x FileName -x Directory
	cachetextconv = true
[diff "ps"]
	textconv = ps2ascii
	cachetextconv = true
	binary = true
[diff "odt"]
	textconv = odt2txt
	cachetextconv = true
[diff "docx"]
	textconv = docx2txt
	cachetextconv = true
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	required = true
[gpg]
	program = gpg2
